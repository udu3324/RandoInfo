{
    "swagger": "2.0",
    "info": {
      "version": "1.0.0",
      "title": "RandoInfo",
      "description": "Get random bits of information!"
    },
    "host": "rando-info.vercel.app",
    "basePath": "/api/v1",
    "schemes": [
      "https"
    ],
    "paths": {
      "/weather": {
        "get": {
          "summary": "Returns the forecast in a random city in the entire world.",
          "produces": [
            "application/json"
          ],
          "responses": {
            "200": {
              "description": "A successful invocation response",
              "schema": {
                "type": "object",
                "properties": {
                  "continent": {
                    "type": "string"
                  },
                  "local_time": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "icon": {
                    "type": "string",
                    "example": "emoji"
                  },
                  "temperature": {
                    "type": "object",
                    "example": {
                      "fahrenheit": "float",
                      "celcius": "float"
                    }
                  },
                  "coordinates": {
                    "type": "object",
                    "example": {
                      "latitude": "float",
                      "longitude": "float"
                    }
                  },
                  "google_maps": {
                    "type": "string",
                    "example": "url"
                  },
                  "open_street_maps": {
                    "type": "string",
                    "example": "url"
                  },
                  "sucess": {
                    "type": "boolean"
                  }
                }
              }
            },
            "404": {
              "description": "A unsuccessful invocation response",
              "schema": {
                "type": "object",
                "properties": {
                  "sucess": {
                    "type": "boolean",
                    "example": false
                  }
                }
              }
            },
            "504": {
              "description": "Gateway timeout/ratelimit",
              "schema": {
                "type": "object",
                "properties": {
                  "error": {
                    "type": "object",
                    "example": {
                      "code": "504",
                      "message": "An error occurred with your deployment"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }